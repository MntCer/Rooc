
(rule
 (target scanner.ml)
 (deps scanner.mll)
 (action
  (chdir
   %{workspace_root}
   (run %{bin:ocamllex} -q -o %{target} %{deps}))))

(menhir
  (modules parser)
  (flags 
          --trace
  )
  )

(env
 (dev
  (flags
   (:standard -warn-error -A))))

(executable
 (public_name Rooc)
 (name Rooc)
 (libraries Rooc))
