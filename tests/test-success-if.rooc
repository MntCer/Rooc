// Test ifelse and string concatenation and return type

fun hello_world(mode:int) -> str
{
    if(mode == 0){
        return "Hello";
    }
    elif(mode == 1){
        return "World";
    }else{
        return "Hello World";
    }
    
}

fun main() -> int
{
    return hello_world(0) + " " + hello_world(1) + " " + hello_world(2);
}