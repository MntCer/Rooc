fun fibonacci(n: int) -> int {
    if n <= 0 {
        return 0;
    }
    if n == 1 {
        return 1;
    } else {
        var a:int = 0;
        var b:int = 1;
        var i:int = 2;

        while (i <= n) {
            var temp:int = a + b;
            a = b;
            b = temp;
            i = i + 1;
        }
        return b;
    }
}

fun main() -> int{
    print_int(fibonacci(0));
    print_int(fibonacci(1));
    print_int(fibonacci(2));
    print_int(fibonacci(10));
    return 0;
}
